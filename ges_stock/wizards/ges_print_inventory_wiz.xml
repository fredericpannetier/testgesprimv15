<?xml version="1.0" encoding="UTF-8"?>
<odoo>    
    <record id="ges_print_inventory_wiz" model="ir.ui.view">
        <field name="name">ges.print_inventory.form</field>
        <field name="model">ges.print.inventory.wiz</field>
        <field name="arch" type="xml">
            <form string="Print inventory">  
            <group>
                <group>
                    <field name="product_ids"/>
                    <field name="category_ids"/>
                </group>
                <group>
                    <field name="lot_detail"/>
                </group>                              
            </group>            
                <footer>
                    <button string="Print" type="object" 
                        name="print_inventory"  default_focus="1" />
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
    <record id="ges_action_print_inventory_wiz" model="ir.actions.act_window">
        <field name="name">Print inventory</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">ges.print.inventory.wiz</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>                    
    </record>    
</odoo>